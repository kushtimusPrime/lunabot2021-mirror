<launch>
    <remap from="odom" to="/rtabmap/odom"/>
    <remap from="map" to="/rtabmap/proj_map"/>
    <remap from="openni_points" to="/rtabmap/local_grid_obstacle" />
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find move_base_kinect)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find move_base_kinect)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find move_base_kinect)/config/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find move_base_kinect)/config/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find move_base_kinect)/config/base_local_planner_params.yaml" command="load" />
    </node>
      
</launch>
